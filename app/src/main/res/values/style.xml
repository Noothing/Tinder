<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="avatarButton_dis">
        <item name="backgroundTint">#3E1C33</item>
        <item name="strokeColor" >#3E1C33</item>
    </style>
    <style name="avatarButton_ac">
        <item name="backgroundTint">#3E1C33</item>
        <item name="strokeColor" >#F6921D</item>
    </style>
    <style name="chipStyle" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor">@drawable/chip_bg</item>
        <item name="android:height">24dp</item>
        <item name="android:textColor">@drawable/chip_text</item>
        <item name="android:textAppearance">@style/chipText</item>
    </style>
    <style name="chipText" parent="TextAppearance.MaterialComponents.Chip">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/baloo_bhai</item>
    </style>
    <style name="bottomNav">
        <item name="bottomNavigationStyle">@style/Widget.MaterialComponents.BottomNavigationView.Colored</item>
        <item name="colorPrimary">#F6921D</item>
        <item name="colorOnPrimary">#B14623</item>
    </style>
</resources>